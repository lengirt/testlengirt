pipeline {
    agent any

    stages {
        stage('Build') {
            steps {
                echo "Start"
                echo "1"
                echo "2"
            }
        }
        stage('Test') {
            steps {
                echo "Start"
                echo "1"
                echo "2"
                sleep 5
            }
        }
        stage('Deploy') {
            steps {
                echo "Start"
                echo "1"
                echo "2"
            }
        }
    }
}